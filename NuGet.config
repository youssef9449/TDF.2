<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
  </packageSources>

  <!-- Completely disable package source mapping to ensure proper resolution -->
  <packageSourceMapping>
    <clear />
  </packageSourceMapping>

  <disabledPackageSources>
    <clear />
  </disabledPackageSources>
  
  <!-- Define fallback folders -->
  <fallbackPackageFolders>
    <clear />
    <add key="UserProfile" value="%USERPROFILE%\.nuget\packages" />
  </fallbackPackageFolders>

  <!-- Suppress specific warnings -->
  <config>
    <add key="treatWarningsAsErrors" value="false" />
    <add key="warningsAsErrors" value="" />
    <add key="noWarn" value="NU1603" />
  </config>
</configuration>