<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
  </packageSources>
  <!-- Disable package source mapping -->
  <disabledPackageSources>
    <clear />
  </disabledPackageSources>
  
  <!-- Define fallback folders -->
  <fallbackPackageFolders>
    <clear />
    <add key="UserProfile" value="%USERPROFILE%\.nuget\packages" />
  </fallbackPackageFolders>
</configuration>